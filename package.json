{
  "name": "oura-ring-cli",
  "version": "1.1.0",
  "description": "CLI tool for accessing Oura Ring data",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "oura": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile oura",
    "build:all": "bun run build:darwin-arm64 && bun run build:darwin-x64 && bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:windows-x64",
    "build:darwin-arm64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile dist/oura-darwin-arm64",
    "build:darwin-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile dist/oura-darwin-x64",
    "build:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile dist/oura-linux-x64",
    "build:linux-arm64": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile dist/oura-linux-arm64",
    "build:windows-x64": "bun build src/index.ts --compile --target=bun-windows-x64 --outfile dist/oura-windows-x64.exe",
    "dev": "bun run --watch src/index.ts"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "oura",
    "oura-ring",
    "health",
    "sleep",
    "cli"
  ],
  "author": "lertsoft",
  "license": "MIT"
}